{
  "dashboard": {
    "id": null,
    "title": "Pure Sound Audio Processing",
    "tags": ["pure-sound", "audio", "processing"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"pure-sound\"}",
            "legendFormat": "Service Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "ðŸ”´ Down"}}, "type": "value"},
              {"options": {"1": {"text": "ðŸŸ¢ Up"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "none"
          },
          "overrides": []
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Processing Jobs",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pure_sound_jobs_total[5m])",
            "legendFormat": "Jobs Created"
          },
          {
            "expr": "rate(pure_sound_jobs_completed_total[5m])",
            "legendFormat": "Jobs Completed"
          },
          {
            "expr": "rate(pure_sound_jobs_failed_total[5m])",
            "legendFormat": "Jobs Failed"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Audio Processing Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(pure_sound_processing_duration_seconds_sum[5m])",
            "legendFormat": "Processing Time"
          },
          {
            "expr": "rate(pure_sound_files_processed_total[5m])",
            "legendFormat": "Files Processed"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Storage Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"pure-sound\"}",
            "legendFormat": "Memory Usage"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1073741824},
                {"color": "red", "value": 2147483648}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h", "2h", "6h", "12h", "1d", "2d", "7d", "30d"]
    }
  }
}